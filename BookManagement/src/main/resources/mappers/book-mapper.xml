<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jhi.book.book.model.mapper.BookMapper">

	<!-- 회원 목록 조회 -->
	<select id="selectBookList">
		SELECT BOOK_NO, BOOK_TITLE, BOOK_WRITER, BOOK_PRICE, REG_DATE
		FROM "BOOK"
	</select>
	
	<!-- 책 등록하기 -->
	<insert id="addBook" parameterType="com.jhi.book.book.model.dto.Book">
		INSERT INTO "BOOK"
		VALUEST(SEQ_TODO_NO.VALUES, #{bookTitle}, #{bookWriter}, #{bookPrice}, DEFAULT)
	</insert>


</mapper>
